{
  "name": "create-tsrpc-app",
  "version": "0.1.0",
  "description": "Quickly create TSRPC full-stack applications",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "switch:ws": "node scripts/switch.js ws",
    "switch:http": "node scripts/switch.js http",
    "sync": "node scripts/sync.js http && node scripts/sync.js ws",
    "prebuild": "node scripts/checkTemplates.js",
    "build": "shx rm -rf dist && rollup -c && node scripts/copyTemplates.js && shx cp package.json LICENSE README.md dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k8w/create-tsrpc-app.git"
  },
  "keywords": [
    "tsrpc",
    "tsrpc-cli",
    "create",
    "full-stack",
    "rpc",
    "tsbuffer",
    "k8w",
    "kingworks",
    "protobuf",
    "grpc"
  ],
  "author": "k8w",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k8w/create-tsrpc-app/issues"
  },
  "homepage": "https://github.com/k8w/create-tsrpc-app#readme",
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/minimist": "^1.2.1",
    "glob": "^7.1.7",
    "rollup": "^2.50.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "shx": "^0.3.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.0",
    "minimist": "^1.2.5",
    "npm-check-updates": "^11.6.0"
  }
}
